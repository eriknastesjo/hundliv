{
  "expo": {
    "name": "proj",
    "slug": "proj",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconImage.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splashImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#0C5D14"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splashImageAdaptiveFront.png",
        "backgroundColor": "#0C5D14"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
